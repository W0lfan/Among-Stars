this.event = function(event, game) {
  let ship = event.ship;
  switch (event.name) {
    case "ui_component_clicked":
      switch (event.id) {
        case "vent":
          ventShip(ship);
          break;
        case "tasks":
          if (showTasks == 0) {
            ship.setUIComponent(taskOne);
            ship.setUIComponent(taskTwo);
            ship.setUIComponent(taskThree);
            showTasks++;
          } else  {
            ship.setUIComponent({id : "taskOne", visible : false});
            ship.setUIComponent({id : "taskTwo", visible : false});
            ship.setUIComponent({id : "taskThree", visible : false});
            showTasks--;
          }
          break;
        case "emergency_meeting_button":
          if (ship.crystals == 20) {
            game.ships[0].set({x:-43 + 5,y:27 });
            game.ships[1].set({x:-43,y:27 + 5});
            game.ships[2].set({x:-43 - 5,y:27});
            game.ships[3].set({x:-43,y:27 - 5});
            game.ships[4].set({x:-43 + 5,y:27 + 5});
            game.ships[5].set({x:-43 - 5,y:27 - 5});
            game.ships[6].set({x:-43 + 2,y:27});
            game.ships[7].set({x:-43,y:27 + 2});
            game.ships[8].set({x:-43+ 2,y:27 - 2});
            game.ships[9].set({x:-43 + 2,y:27 + 2});
            showEmergencyButton(ship);
            emergency++;
          }
          break;
      }
      break;
  case "ship_destroyed":
    if (ship !== null) {
      shipGotDestroyed(ship);
      game.setObject({
        id: "killeCrewmte",
        type: killedCrewmate,
        position: {x:ship.x,y:ship.y,z:-10},
        rotation: {x:0,y:0,z:0},
        scale: {x:2,y:2,z:2}
      }) ;
    }
  }
};


