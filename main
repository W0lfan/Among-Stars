this.tick = function(game) {
  if (game.step % 15 == 0) {
    for (let ship of game.ships) {
      appearEmergencyMeetingButton(ship); //Emergency button appears if...
      scoreSet(ship); //Set the score to 0.
      ship.setUIComponent(hideMiningBar);
      setStats(ship);
        if (!ship.custom.init) {//6
          ship.custom.init = true; //When the ship spawns.
          ship_spawn(ship);
          if (ImpostorChances[~~(Math.random()*ImpostorChances.length)] == "yes" && numberOfImpostors <= 1) {//1/2 chance to be impostor.
            putInImpostorTeam(ship);// Put in impostor's team.
            numberOfImpostors++;// +1 tomake tht only one impostor can join impostor's again.
            ship_impasta_join(ship); //Impostor's ship.
          }
          else {
            putInCrewTeam(ship); // Put in crewmat's team.
            ship_not_impasta_join(ship);// Gives non-impostor ship.
          }
          
        }
      }
    }
    if (game.step % 3600/2 == 0 ) {
      ship.setUIComponent({id: "infos", visible: false}); //Hide the informations at the start of the game.
    }
    if (game.step % 60 == 0) {
      for (let ship of game.ships) {
          if (time !== -1) {
            var timer = {
              id: "timer",
              position: [40,39,70,50],
              visible: true,
              components: [
                { type: "text",position:[5,75,80,30],value: time,color:"#1CECEC"},
                ]
            }; //Timer stuff
            time--;
            ship.setUIComponent(timer); // Set the timer.
            ship.setUIComponent(waiting); //Set the waiting sentence.
            ship.setUIComponent(credits); //Set the credits sentence.
            blockedShipBecauseYouHaveToWait(ship); //You can't move, sad.
          } else {
            ship.setUIComponent(infos); //Show infos's componenet.
            notBlockedAnymoreBecauseYouDontHaveToWaitAnymore(ship); //You can now move, cool.
            ship.setUIComponent({id: "waiting", visible: false}); //Hide the waiting component.
            ship.setUIComponent({id: "credits", visible: false}); //Show the credits componenet.
            ship.setUIComponent({id: "timer", visible: false}); //Show the timer.
            if (ship.team == CREWMATE) {
              ship.setUIComponent(tasks); //Show the task button.
            } else {
              ship.setUIComponent(vent); //Show the vent button.
          }
        }
        /*if (time2 == -1) {
          emergency--;
        }
        if (emergency == 1 && time2 !== -1) {
            ship.setUIComponent(timer2);
            blockedShipBecauseYouHaveToWait(ship);
            var timer2 = {
              id: "timer2",
              position: [40,39,70,50],
              visible: true,
              components: [
                { type: "text",position:[5,75,80,30],value: time2,color:"#1CECEC"},
                ]
            }; //Timer stuff
            time2--;
          } else {
            notBlockedAnymoreBecauseYouDontHaveToWaitAnymore(ship);
            ship.setUIComponent({id: "timer2", visible: false});
          }*/
      }
      }
};//1


